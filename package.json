{
  "name": "Scrbbl",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client": "npm start --prefix client",
    "server": "nodemon ./bin/www",
    "start": "npm run build",
    "prebuild": "cd client && npm i && npm run build",
    "build": "node ./bin/www",
    "dev": "concurrently \"npm run server\" \"npm run client\""
